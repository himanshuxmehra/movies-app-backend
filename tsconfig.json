{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "outDir": "dist/js",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node"],
    "resolveJsonModule": true,
  },
  "include": [ "**/*" ],
  "exclude": [ "node_modules","dist" ],
}